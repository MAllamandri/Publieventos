<div class="row">
    <div class="col-md-7 partipants-panel panel panel-warning nano">
        <div class="nano-content">
            <div class="panel-heading">
                <p class="label-lobster" style="font-size: 25px; text-align: center;" data-bind="text: StandbyTitle"></p>
            </div>
            <p style="font-size: 15px; margin-top: 10%;" data-bind="visible: Standby().length == 0" class="not-found-message">No Hay Usuarios Invitados</p>

            <div data-bind="foreach: Standby">

                <div class="col-md-11 panel-body" style="margin-top: 15px">
                    <div class="col-md-3" style="display: inline; height: 40px; width: 70px;">
                        <a href="#" data-bind="click: Profile">
                            <img data-bind="attr: { src: ImageProfile, title: UserName }" style="cursor: pointer" alt="Perfil del usuario" class="participants-image-profile img-circle right" data-src="holder.js/40x40">
                        </a>
                    </div>
                    <div class="col-md-8" style="text-align: center">
                        <p class="participants-user" data-bind="text: FullName"></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
