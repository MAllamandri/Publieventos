<div data-bind="foreach: Events">
    <div class="col-md-12" style="margin-bottom: 5%;">
        <div class="col-md-12 mosaic">
            <div class="row-fluid subtitle">
                <div class="span12">
                    <div class="span4">
                        <div class="tl-circ" style="padding-top: 10px; padding-bottom: 10px;">
                            <img data-bind="attr: { src: PicturePath }" style="width: 100%; height: 100%;" alt="" class="img-responsive" data-src="holder.js/1000x500">
                        </div>
                    </div>
                    <div class="span8">
                        <h4 class="font-text" style="color: #c25b4e" data-bind="text: Title"></h4>
                        <p>
                            <p class="font-text" style="display: inline; font-size: 15px">Fecha:</p>
                            <strong class="font-text" style="display: inline; font-size: 15px;" data-bind="text: EventDate"></strong>
                        </p>
                        <p>
                            <p class="font-text" style="display: inline; font-size: 15px;">Hora:</p>
                            <strong class="font-text" style="display: inline; font-size: 15px;" data-bind="text: EventTime"></strong>
                        </p>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="span12">
                                    <p class="font-text" style="font-size: 15px" data-bind="text: Description"></p>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid" style="margin-bottom: 10px">
                            <div class="span12">
                                <strong style="color: black">Administrador</strong>
                            </div>
                            <div class="col-md-4">
                                <a href="#" data-bind="click: UserProfile" style="display: inline; margin-right: 15px;">
                                    <img data-bind="attr: { src: ImageProfile, title: UserName }" style="cursor: pointer" alt="Perfil del usuario" class="participants-image-profile img-circle right" data-src="holder.js/40x40">
                                </a>
                                <p style="display: inline" class="participants-user" data-bind="text: UserName"></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div class="span12">
                        <p class="font-text" style="white-space: pre-line; font-size: 15px; margin-top: 5px;" data-bind="text: Detail"></p>
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="col-md-12">
                    <div style="margin-bottom: 40px !important">
                        <a href="#" style="float: right">Desactivar Evento</a>
                        <a href="#" style="float: right; margin-right: 15px;">Ignorar Reporte</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<p data-bind="visible: Events().length == 0" style="font-size: 20px; margin-top: 30px;" class="not-found-message">No Hay Eventos Reportados</p>